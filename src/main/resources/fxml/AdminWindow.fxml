<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TextField?>
<AnchorPane prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.kawka.appobrona.controllers.AdminWindowController">
   <children>
      <Label layoutX="311.0" layoutY="14.0" text="AdminWindow" />
      <Button layoutX="607.0" layoutY="645.0" mnemonicParsing="false" onAction="#back" text="Wstecz" />
      <Button layoutX="30.0" layoutY="45.0" mnemonicParsing="false" onAction="#readDatabase" text="Wczytaj cała baze" />
      <Button layoutX="316.0" layoutY="45.0" mnemonicParsing="false" onAction="#actionOpenCreateEmployeeWindow"
              text="Stwórz pracownika" />
      <Button layoutX="30.0" layoutY="110.0" mnemonicParsing="false" onAction="#actionReadCustomers"
              prefHeight="25.0" prefWidth="110.0" text="Wczytaj pracowników" />
      <Button layoutX="445.0" layoutY="45.0" mnemonicParsing="false" onAction="#actionOpenUpdateEmployeeWindow"
              text="Modyfikuj pracownika" />


      <TextField fx:id="idFieldId" layoutX="30.0" layoutY="160.0" prefWidth="110.0" promptText="ID" />
      <TextField fx:id="idFieldFirstName" layoutX="160.0" layoutY="110.0" prefWidth="110.0" promptText="Imie" />
      <TextField fx:id="idFieldLastName" layoutX="160.0" layoutY="160.0" prefWidth="110.0" promptText="Nazwisko" />
      <TextField fx:id="idFieldStatus" layoutX="290.0" layoutY="110.0" prefWidth="110.0" promptText="Status" />
      <TextField fx:id="idFieldLogin" layoutX="290.0" layoutY="160.0" prefWidth="110.0" promptText="Login" />
      <TextField fx:id="idFieldPassword" layoutX="420.0" layoutY="110.0" prefWidth="110.0" promptText="Hasło" />

      <TableView fx:id="employeeTableView" layoutX="18.0" layoutY="204.0" prefHeight="415.0" prefWidth="663.0">
         <columns>
            <TableColumn fx:id="id" prefWidth="30.0" text="ID" />
            <TableColumn fx:id="firstName" prefWidth="70.0" text="Imie" />
            <TableColumn fx:id="lastName" prefWidth="70.0" text="Nazwisko" />
            <TableColumn fx:id="status" prefWidth="70.0" text="Status" />
            <TableColumn fx:id="login" prefWidth="70.0" text="Login" />
            <TableColumn fx:id="password" prefWidth="70.0" text="Hasło" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
